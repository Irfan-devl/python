a=[20,123,56,21,55,66,88,120,34,67,90]
b=[]
c=[]
for i in range(len(a)):
    b.append(str(a[i]))
for i in range(len(b)):
    d=0
    for j in range(len(b[i])):
        d=d+int(b[i][j])
    c.append(d)
for i in range(len(c)):
    if(c[i]%2==0):
        print(a[i],":True")
    else:
        print(a[i],":False")
