a=int(input("enter the number: "))
b=int(input("enter the number: "))
c=int(input("enter the number: "))
a1=list(str(a))
b1=list(str(b))
c1=list(str(c))
a2=[]
b2=[]
c2=[]
for i in range(4):
    a2.append(int(a1[i]))
    b2.append(int(b1[i]))
    c2.append(int(c1[i]))
x=max(a2)+max(b2)+max(c2)
y=min(a2)+min(b2)+min(c2)
print(x+y)


    
    
